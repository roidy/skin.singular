<?xml version="1.0" encoding="utf-8"?>
<window>
    <controls>
        <control type="group">
            <animation effect="slide" start="0,-108" end="0,0" time="200">WindowOpen</animation>
            <animation effect="slide" start="0,-108" end="0,0" time="200">VisibleChange</animation>
            <animation effect="slide" start="0,0" end="0,-108" time="200">WindowClose</animation>
            <visible>!Window.IsActive(videoosd)</visible>
            <top>0</top>
            <control type="slider" id="401">
                <left>0</left>
                <top>65</top>
                <width>100%</width>
                <height>26</height>
                <texturesliderbar colordiffuse="00FFFFFF">progress/ProgressBackground450.png</texturesliderbar>
                <textureslidernib colordiffuse="Primary">progress/ProgressBarMid16.png</textureslidernib>
                <textureslidernibfocus colordiffuse="Primary">general/White.png</textureslidernibfocus>
                <!-- <visible>Player.SeekEnabled + !Player.ChannelPreviewActive</visible> -->
            </control>

            <control type="label" id="402">
                <left>0</left>
                <top>0</top>
            </control>
        </control>
    </controls>
</window>